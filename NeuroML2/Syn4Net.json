                    "neuroml2_source_file": "fourPathwaySyn.synapse.nml"
                }
            }
        ], 
        "input_sources": [
            {
                "stim1": {
                    "pynn_input": "DCSource", 
                    "parameters": {
                        "start": "stim1_delay", 
                        "stop": "stim1_delay+5", 
                        "amplitude": 0.4
                    }
                }
            }, 
            {
                "stim2": {
                    "pynn_input": "DCSource", 
                    "parameters": {
                        "start": "stim2_delay", 
                        "stop": "stim2_delay+5", 
                        "amplitude": 0.4
                    }
                }
            }
        ], 
        "regions": [
            {
                "region1": {
                    "x": 0.0, 
                    "y": 0.0, 
                    "z": 0.0, 
                    "width": 1000.0, 
                    "height": 100.0, 
                    "depth": 1000.0
                }
            }
        ], 
        "populations": [
            {
                "pop0": {
                    "size": 1, 
                    "component": "spkArr1", 
                    "properties": {
